<h1>Request Group Rate</h1>
<form>
  <input type="hidden" name="referrer" /><br />
  Name: <input type="text" id="filedName" name="name" /><br />
  Group size:<input type="text" name="groupSize"><br />
  Email: <input type="email" name="email" ><br>
  <input type="submit" value="Submit">
</form>
<script>
  $(document).ready(function(){
      var reg = /(?=([^f\=]+))[\w-]+/g;
      var list = window.location.hash.match(reg);
        $('input[name="referrer"]').val(document.referrer || list&&list.length>0?list[0]:'');
  });  
</script>